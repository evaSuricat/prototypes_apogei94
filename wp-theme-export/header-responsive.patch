+  }
+  .apogei-header__links a {
+    display: block;
+    padding: var(--apogei--space--md, 1rem) 0;
+    font-size: var(--wp--preset--font-size--body-lg, 1.125rem);
+  }
+  .apogei-header__links a[aria-current=page]::after {
+    display: none;
+  }
+}
+
+.apogei-header__nav-actions {
+  display: none;
+}
+@media (max-width: 64rem) {
+  .apogei-header__nav-actions {
+    display: flex;
+    flex-direction: column;
+    gap: var(--apogei--space--sm, 0.75rem);
+    margin-top: auto;
+    padding-top: var(--apogei--space--xl, 2rem);
+  }
+  .apogei-header__nav-actions .wp-block-button__link {
+    width: 100%;
+    text-align: center;
+  }
+}
+
+.apogei-header__actions {
+  display: flex;
+  gap: var(--apogei--space--sm, 0.75rem);
+}
+@media (max-width: 64rem) {
+  .apogei-header__actions {
+    grid-column: 3;
+    justify-self: end;
+  }
+  .apogei-header__actions .apogei-header__btn--outline {
+    display: none;
+  }
+}
+
+.apogei-header__overlay {
+  position: fixed;
+  inset: 0;
+  background-color: rgba(0, 0, 0, 0);
+  z-index: calc(var(--apogei--z--overlay, 1300) - 1);
+  pointer-events: none;
+  transition: background-color var(--apogei--duration--slower, 350ms) var(--apogei--easing--default, ease);
+}
+.apogei-header.is-menu-open .apogei-header__overlay {
+  background-color: rgba(0, 0, 0, 0.4);
+  pointer-events: auto;
+}
+
+@media (prefers-reduced-motion: reduce) {
